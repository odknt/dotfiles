"amixer set Master 5%+"
    XF86AudioRaiseVolume
"amixer set Master 5%-"
    XF86AudioLowerVolume
"amixer set Master toggle"
    XF86AudioMute

"mpc toggle"
    XF86AudioPlay
"mpc stop"
    XF86AudioStop
"mpc prev"
    XF86AudioPrev
"mpc next"
    XF86AudioNext

"rofi -show run"
    XF86Search
"${BROWSER}"
    XF86HomePage
